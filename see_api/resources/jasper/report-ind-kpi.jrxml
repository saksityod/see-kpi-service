		<queryString language="SQL">
			<![CDATA[SELECT
	"Actual" AS type_name,
	mon.period_id,
	mon.`year`,
	mon.item_id,
	mon.appraisal_month_name,
	mon.org_id ,
	SUM( mon.actual_value ) AS actual_target_value ,
	mon.appraisal_month_no
FROM
	monthly_appraisal_item_result mon
INNER JOIN appraisal_item_result re ON
	mon.item_result_id = re.item_result_id
WHERE
	mon.item_id = $P{item}
	AND mon.year = 2018
	AND mon.period_id = $P{period}
	AND mon.org_id = $P{org}
	AND mon.emp_id = $P{emp}
	AND mon.emp_result_id = $P{emp_result}
GROUP BY
	mon.period_id,
	mon.`year`,
	mon.item_id,
	mon.appraisal_month_name,
	mon.org_id,
	re.target_value ,
	mon.appraisal_month_no
UNION SELECT
	"Target" AS type_name,
	mon.period_id,
	mon.`year`,
	mon.item_id,
	mon.appraisal_month_name,
	mon.org_id ,
	re.target_value ,
	mon.appraisal_month_no
FROM
	monthly_appraisal_item_result mon
INNER JOIN appraisal_item_result re ON
	mon.item_result_id = re.item_result_id
WHERE
	mon.item_id = $P{item}
	AND mon.year = 2018
	AND mon.period_id = $P{period}
	AND mon.org_id = $P{org}
	AND mon.emp_id = $P{emp}
	AND mon.emp_result_id = $P{emp_result}
GROUP BY
	mon.period_id,
	mon.`year`,
	mon.item_id,
	mon.appraisal_month_name,
	mon.org_id,
	re.target_value ,
	mon.appraisal_month_no
ORDER BY
	8]]>
		</queryString>
