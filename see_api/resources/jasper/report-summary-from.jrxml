	<parameter name="subreport_path" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT qhead.data_header_id ,qtail.section_id,
	IF(q.answer_type_id = 7, 'comment', 
		IF(q.answer_type_id != 7 AND qsec.is_cust_search = 1, 'crosstab',
			IF(q.answer_type_id != 7 AND qsec.is_cust_search = 0, 'table', 'table')
 		)
 	) display_type
FROM questionaire_data_detail qtail
INNER JOIN questionaire_data_header qhead ON qhead.data_header_id = qtail.data_header_id
INNER JOIN question q ON q.question_id = qtail.question_id AND q.section_id = qtail.section_id
INNER JOIN questionaire_section as qsec on qsec.questionaire_id = qhead.questionaire_id AND qsec.section_id = qtail.section_id
WHERE qhead.data_header_id = $P{param_data_header_id} 
GROUP BY qtail.section_id]]>
	</queryString>
