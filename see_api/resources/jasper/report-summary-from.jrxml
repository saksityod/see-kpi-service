	<subDataset name="LastPageQry" uuid="77cdfddb-c446-48b7-b8b3-6f28bb6d8105">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="pmtl"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="349"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="640"/>
		<parameter name="param_data_header_id" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[SELECT 
	IFNULL(MAX(acknowledge_by), '.......................................') acknowledge_by,
	IFNULL(MAX(complete_by), '.......................................') complete_by,
	IFNULL(MAX(acknowledge_date), '.......................................') acknowledge_date,
	IFNULL(MAX(complete_date), '.......................................') complete_date
FROM(
	SELECT
		(
			SELECT CONCAT(aemp.emp_first_name, ' ', aemp.emp_last_name) 
			FROM employee_snapshot aemp
			WHERE aemp.emp_snapshot_id = IF(stag.to_stage_id = 4, stag.to_emp_snapshot_id, 0)
		) acknowledge_by,
		(
			SELECT CONCAT(cemp.emp_first_name, ' ', cemp.emp_last_name) 
			FROM employee_snapshot cemp
			WHERE cemp.emp_snapshot_id = IF(stag.to_stage_id = 3, stag.from_emp_snapshot_id, 0)
		) complete_by,
		IF(stag.to_stage_id = 4, DATE_FORMAT(stag.created_dttm, "%d/%m/%Y %H:%i:%s"), NULL) acknowledge_date,
		IF(stag.to_stage_id = 3, DATE_FORMAT(stag.created_dttm, "%d/%m/%Y %H:%i:%s"), NULL) complete_date
	FROM questionaire_data_stage stag
	WHERE stag.data_header_id = $P{param_data_header_id} 
	AND stag.to_stage_id IN(3,4)
)mq]]>
		</queryString>
		<field name="acknowledge_by" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="acknowledge_by"/>
		</field>
		<field name="acknowledge_date" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="acknowledge_date"/>
		</field>
		<field name="complete_by" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="complete_by"/>
		</field>
		<field name="complete_date" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="complete_date"/>
		</field>
	</subDataset>
	<parameter name="param_questionaire_type" class="java.lang.String"/>
	<parameter name="param_data_header_id" class="java.lang.String"/>
	<parameter name="param_questionaire_date" class="java.lang.String"/>
	<parameter name="param_assessor" class="java.lang.String"/>
