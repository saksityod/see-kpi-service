	<queryString language="SQL">
		<![CDATA[SELECT emp.emp_code
, emp.emp_name
, `level`.appraisal_level_name
, po.position_name
, (select emp1.emp_name from employee emp1 WHERE emp1.emp_code = emp.chief_emp_code ) as chief_emp_name
, (case when emp.has_second_line = 0 then ''
	else (select emp_name from employee e1 where e1.emp_code = (
		select e.chief_emp_code 
		from employee e
		where e.emp_code = emp.chief_emp_code)
	)end) as chief_second
, org.org_name
, i.item_name
, s.structure_name
, (CASE WHEN (f.form_id = 1 OR f.form_id = 2) THEN 'Target Value' ELSE 'Max Value' END)AS name_one
, COALESCE((CASE WHEN (f.form_id = 1 OR f.form_id = 2) THEN air.target_value ELSE air.max_value END),'') AS one
, (CASE WHEN (f.form_id = 1 OR f.form_id = 3) THEN 'Actual Value' ELSE 'First Score' END) AS name_two
, COALESCE((CASE WHEN (f.form_id = 1 OR f.form_id = 3) THEN air.actual_value ELSE air.first_score END),'') AS two
, (CASE 
  WHEN f.form_id = 1 THEN 'Score'
  WHEN f.form_id = 2 THEN 'Second Score'
  ELSE 'Deduct Score/Unit'
 END) AS name_three
, COALESCE((CASE 
  WHEN f.form_id = 1 THEN air.score
  WHEN f.form_id = 2 THEN air.second_score
  ELSE air.deduct_score_unit
 END),'') AS three
, '%Weight' AS name_four
, COALESCE((CASE WHEN (f.form_id = 1 OR f.form_id = 2) THEN air.weight_percent ELSE 0 END),'')  AS four
, 'Weight Score' AS name_five
, COALESCE(air.weigh_score,'') AS five
, COALESCE ( sr.weigh_score,'') AS weigh_score
, COALESCE(er.result_score,'') AS result_score

FROM appraisal_item_result air
INNER JOIN appraisal_period apr ON apr.period_id = air.period_id 
inner join employee emp on air.emp_id = emp.emp_id
inner join appraisal_level level on air.level_id = level.level_id
inner join position po on po.position_id = air.position_id
inner join org org on org.org_id = air.org_id
INNER JOIN appraisal_item i ON air.item_id = i.item_id
INNER JOIN appraisal_structure s ON i.structure_id = s.structure_id
INNER JOIN form_type f ON s.form_id = f.form_id
INNER JOIN structure_result sr ON sr.period_id = air.period_id
 AND sr.emp_id = air.emp_id
 AND sr.structure_id = s.structure_id
LEFT JOIN emp_result er ON er.period_id = air.period_id
 AND er.emp_id = air.emp_id
 AND er.org_id = air.org_id
 AND er.position_id = air.position_id
 AND er.level_id = air.level_id
WHERE air.period_id = $P{param_period} 
AND air.org_id = $P{param_org_id}
AND apr.appraisal_year =  $P{param_year}
and org.level_id =  $P{param_level_org} 
and air.level_id = $P{param_level_emp}]]>
	</queryString>
