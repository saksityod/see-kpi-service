	<parameter name="param_org" class="java.lang.String"/>
	<parameter name="param_period" class="java.lang.String"/>
	<parameter name="param_level_org" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="param_user" class="java.lang.String"/>
	<parameter name="param_emp" class="java.lang.String"/>
	<parameter name="param_position" class="java.lang.String"/>
	<parameter name="param_level" class="java.lang.String"/>
	<queryString>
		<![CDATA[-- comment
SELECT
ER.*,
CONCAT(ap.appraisal_period_desc,'/',ap.appraisal_year) as h,
ap.appraisal_period_desc,
ap.appraisal_year,
o.org_name,
Emp.emp_id,
Emp.emp_code,
Emp.emp_name,
Emp.position_id,
air.item_id,
air.item_name,
air.level_id,
air.org_id,
CASE WHEN air.kpi_type_id=2 THEN"#e6e6e6" ELSE "#ffffff" END as color_status,
CASE WHEN ai.value_type_id = 1 THEN ">/=" ELSE "</=" end as value_type_id,
(CASE WHEN air.percent_achievement >= 100 THEN '#0033cc' ELSE '#ff0000' END) as color_percent_achievement,
air.actual_value,
u.uom_name,
air.target_value,
air.percent_achievement,
air.period_id,
air.item_result_id,
re.re
FROM
appraisal_item_result AS air
INNER JOIN appraisal_period AS ap ON air.period_id = ap.period_id
INNER JOIN appraisal_item AS ai ON air.item_id = ai.item_id
INNER JOIN uom AS u ON ai.uom_id = u.uom_id
INNER JOIN org AS o ON o.org_id = air.org_id
INNER JOIN employee Emp ON air.emp_id = Emp.emp_id
LEFT JOIN emp_result emr ON air.emp_result_id = emr.emp_result_id
LEFT JOIN (SELECT count(reason_name) as re , item_result_id
	FROM reason 
	GROUP BY item_result_id
	)re ON re.item_result_id = air.item_result_id
INNER JOIN (SELECT org.org_id
	, org.org_code
	, org.org_name
	, org.parent_org_code
	, (SELECT org1.org_name FROM org org1 WHERE org1.org_code = org.parent_org_code) AS parent_org_name
	, AP.appraisal_period_desc
	, ER.period_id
	, ER.result_score
	, Emp.emp_id
	, Emp.emp_code
	, Emp.emp_name
	, ER.emp_result_id
	, Po.position_id
	, Po.position_name
	, AL.appraisal_level_name 
	, ER.level_id
	FROM emp_result ER
	INNER JOIN appraisal_period AP ON ER.period_id = AP.period_id
	INNER JOIN employee Emp ON ER.emp_id = Emp.emp_id
	INNER JOIN org ON Emp.org_id = org.org_id
	INNER JOIN position Po ON Emp.position_id = Po.position_id
	INNER JOIN appraisal_level AL ON Emp.level_id = AL.level_id 
	WHERE ER.appraisal_type_id = '2' 
		AND (ER.period_id = $P{param_period} OR  ISNULL($P{param_period}) = 1 OR ""=$P{param_period})
		AND (ER.org_id IN($P!{param_org})) 
		AND (ER.emp_id = $P{param_emp} OR ISNULL($P{param_emp}) = 1 OR ""=$P{param_emp})
		AND (ER.position_id =  $P{param_position}  OR ISNULL($P{param_position}) = 1 OR ""=$P{param_position})
