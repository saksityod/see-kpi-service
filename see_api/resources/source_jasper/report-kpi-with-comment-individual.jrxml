	<subDataset name="table" uuid="9e6e2b35-deee-4496-8dc2-c9bcde0bcaf8">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="see-kpi-demo"/>
		<parameter name="param_org" class="java.lang.String"/>
		<parameter name="param_period" class="java.lang.String"/>
		<parameter name="param_item" class="java.lang.String"/>
		<parameter name="param_level" class="java.lang.String"/>
		<parameter name="param_emp" class="java.lang.String"/>
		<parameter name="param_position" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
d.appraisal_month_no,
d.month_name,
d.target_value,
d.cds_value,
d.percent_achievement,
CASE WHEN d.kpi_type_id=2 THEN"#e6e6e6" ELSE "#ffffff" END as color_status,
(CASE WHEN d.percent_achievement >= 100 THEN '#0033cc' ELSE '#ff0000' END) as color_percent_achievement,
concat("#",COALESCE((
	SELECT IFNULL (
	(SELECT rt.color_code FROM result_threshold rt
	WHERE rt.result_type = (SELECT result_type FROM system_config)
	AND (rt.begin_threshold <= d.percent_achievement and rt.end_threshold >= d.percent_achievement)
	),  
	(SELECT (CASE WHEN d.percent_achievement < m.min_begin THEN max(m.color_min) 
	ELSE max(m.color_max) END) as color
	FROM (
		SELECT (case when rt.begin_threshold = val.min_begin then rt.color_code else null end) AS color_min
		, (case when rt.end_threshold = val.max_end then rt.color_code else null end) as color_max
		, val.min_begin, val.max_end
		FROM result_threshold rt
		CROSS JOIN (SELECT min(rt.begin_threshold) as min_begin
			, max(rt.end_threshold) as max_end
			FROM result_threshold rt
			WHERE rt.result_type = (SELECT result_type FROM system_config)) val 
		WHERE rt.result_type = (SELECT result_type FROM system_config)
	)m)) as color
	),'CACACA')) as color_code
FROM
( SELECT 
cr.appraisal_month_no,
air.kpi_type_id,
MONTHNAME(CONCAT(cr.`year`,'-',cr.appraisal_month_no,'-01')) AS month_name,
100 as target_value,
-- (CASE WHEN ai.function_type = 3 THEN air.target_value ELSE mon.target_value  END) AS target, 
mon.actual_value as cds_value,
ROUND(CASE WHEN (ai.value_type_id = 1)
     THEN
       (
        CASE WHEN (air.target_value = 0 and cr.cds_value = 0) THEN 100
           WHEN (air.target_value = cr.cds_value) THEN 100
           WHEN (air.target_value IS NULL) THEN 0
           WHEN (air.target_value = 0) THEN 0
           ELSE (cr.cds_value / air.target_value) * 100
           END
       )
     ELSE
       (
        CASE WHEN (air.target_value = 0 and cr.cds_value = 0) THEN 100
           WHEN (air.target_value = cr.cds_value) THEN 100
           WHEN (air.target_value IS NULL) THEN 0
           WHEN (air.target_value = 0) THEN 0
           ELSE  100 + (((air.target_value - cr.cds_value ) / air.target_value )*100)
           END
       )
			 END,2) as percent_achievement 
FROM
cds_result AS cr
INNER JOIN kpi_cds_mapping AS kcm ON kcm.cds_id = cr.cds_id AND kcm.item_id =  $P{param_item}
INNER JOIN appraisal_item_result AS air ON air.period_id = cr.period_id AND air.org_id = cr.org_id AND air.item_id = kcm.item_id AND air.level_id = cr.level_id
INNER JOIN appraisal_item AS ai ON kcm.item_id = ai.item_id
INNER JOIN monthly_appraisal_item_result AS mon ON mon.item_result_id = air.item_result_id AND mon.item_id = ai.item_id 
	AND mon.appraisal_month_no = cr.appraisal_month_no
INNER JOIN (select cr.appraisal_month_no,
			(CASE WHEN ai.function_type = 3 THEN air.target_value ELSE mon.target_value  END) AS target
			FROM
			cds_result AS cr
			INNER JOIN kpi_cds_mapping AS kcm ON kcm.cds_id = cr.cds_id AND kcm.item_id = $P{param_item}
			INNER JOIN appraisal_item_result AS air ON air.period_id = cr.period_id AND air.org_id = cr.org_id AND air.item_id = kcm.item_id AND air.level_id = cr.level_id
			INNER JOIN appraisal_item AS ai ON kcm.item_id = ai.item_id
			INNER JOIN monthly_appraisal_item_result AS mon ON mon.item_result_id = air.item_result_id AND mon.item_id = ai.item_id 
				AND mon.appraisal_month_no = cr.appraisal_month_no
			WHERE cr.appraisal_type_id = 2
			AND cr.period_id = $P{param_period}
			AND cr.org_id = $P{param_org}
			AND cr.level_id = $P{param_level}
			AND air.emp_id = $P{param_emp} 
			AND air.position_id = $P{param_position} 
			GROUP BY cr.appraisal_month_no) tar ON tar.appraisal_month_no = cr.appraisal_month_no
WHERE cr.appraisal_type_id = 2
AND cr.period_id =  $P{param_period}
AND cr.org_id = $P{param_org}
AND cr.level_id = $P{param_level}
AND air.emp_id = $P{param_emp} 
AND air.position_id = $P{param_position} 
GROUP BY cr.appraisal_month_no
)d
ORDER BY d.appraisal_month_no]]>
		</queryString>
		<field name="appraisal_month_no" class="java.lang.Integer">
			<property name="com.jaspersoft.studio.field.label" value="appraisal_month_no"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="cds_result"/>
