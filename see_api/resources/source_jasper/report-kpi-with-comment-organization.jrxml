ap.appraisal_year,
o.org_name,
air.item_id,
air.item_name,
air.level_id,
air.org_id,
CASE WHEN air.kpi_type_id=2 THEN"#e6e6e6" ELSE "#ffffff" END as color_status,
CASE WHEN ai.value_type_id = 1 THEN ">/=" ELSE "</=" end as value_type_id,
(CASE WHEN air.percent_achievement >= 100 THEN '#0033cc' ELSE '#ff0000' END) as color_percent_achievement,
air.actual_value,
u.uom_name,
air.target_value,
air.percent_achievement,
air.period_id,
air.item_result_id,
re.re
FROM
appraisal_item_result AS air
INNER JOIN appraisal_period AS ap ON air.period_id = ap.period_id
INNER JOIN appraisal_item AS ai ON air.item_id = ai.item_id
INNER JOIN uom AS u ON ai.uom_id = u.uom_id
INNER JOIN org AS o ON o.org_id = air.org_id
LEFT JOIN emp_result emr ON air.emp_result_id = emr.emp_result_id
LEFT JOIN (SELECT count(reason_name) as re , item_result_id
	FROM reason 
	GROUP BY item_result_id
	)re ON re.item_result_id = air.item_result_id
WHERE air.org_id IN($P!{param_org})
AND ap.period_id = $P{param_period} 
AND (air.level_id = $P{param_level_org} or '' =  $P{param_level_org} or isnull($P{param_level_org}) = 1)
AND emr.appraisal_type_id = 1
ORDER BY air.org_id ,ai.perspective_id asc ,ai.kpi_id asc ,ai.item_name asc]]>
